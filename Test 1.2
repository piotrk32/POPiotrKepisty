import java.util.Random;
import java.util.Scanner;

public class Main
{
	public static void main(String[] args) 
	
	{
		Random rand = new Random();
		int n = rand.nextInt(100);
		int tab[] = new int[n];
		int min = -50;
		int max = 50;
		int ile = 0;
		
		for (int i = 0; i<=tab.length-1 ; i++)
		
		{
		   
		   int s = rand.nextInt((max-min)+min);
		   tab[i]= s;
		   System.out.print(tab[i] + " " + "\n");
		   if (tab[i] == tab[i])
		   {
		       ile++;
		   }
		}
		System.out.println("Ilosc wystapien najpopularniejszego elementu tablicy : " + ile);
		
	}
}
